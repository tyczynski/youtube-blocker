{
  "name": "youtube-blocker",
  "version": "1.0.0",
  "description": "Browser extension to block unwanted channels.",
  "main": "index.js",
  "scripts": {
    "webpack:build": "webpack --config ./scripts/webpack.prod.js",
    "webpack:dev": "webpack -w --config ./scripts/webpack.dev.js",
    "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
    "prettify": "prettier --write \"src/**/*.ts*\"",
    "test": "jest",
    "web-ext:dev": "web-ext run --source-dir dist --start-url https://youtube.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tyczynski/youtube-blocker.git"
  },
  "keywords": [
    "react",
    "typescript",
    "chrome",
    "extension",
    "boilerplate"
  ],
  "author": "Przemysław Tyczyński",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tyczynski/youtube-blocker/issues"
  },
  "homepage": "https://github.com/tyczynski/youtube-blocker#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@types/chrome": "0.0.117",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.10",
    "typescript": "^4.0.5",
    "webextension-polyfill-ts": "^0.21.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.3.0"
  },
  "dependencies": {
    "dayjs": "^1.9.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "web-ext": "^5.3.0"
  }
